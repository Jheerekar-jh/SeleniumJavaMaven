package JavaPackage;

public class DataTypeAndVariable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int myNum=5;
		String website ="This is Jaswanth";
		char letter='r';
		double dec=5.99;

		System.out.println(myNum + " "+website + " "+letter +" "+ dec);	
		//Creating Array
		int[] arr= new int[5];
		arr[0]=1;
		arr[1]=2;
		arr[2]=4;
		arr[3]=3;
		arr[4]=6;
		
		int[] arr2= {1,2,3,4,5,7,8,9,10};
		
		for(int i=0; i < arr.length; i++)
		{
			System.out.println(arr[i]);
		}
		String[] a= {"Jaswanth","Heerekar","Nyra","Amulya"};
		
		for(String s:a)
		{
			System.out.println(s);
		}
		
		for(int j=0;j<arr2.length;j++)
		{
			if(arr2[j]%2==0)
			{
				System.out.println(arr2[j]);
			}
		}
		
		


	}

}
